{
  "name": "on-nextjs-project",
  "version": "2.0.0",
  "private": true,
  "author": {
    "name": "ON Associates",
    "email": "dev@madebyon.com",
    "url": "https://madebyon.com"
  },
  "maintainers": [
    {
      "name": "ON Associates",
      "email": "dev@madebyon.com",
      "url": "https://madebyon.com"
    }
  ],
  "engines": {
    "node": ">=20.x",
    "yarn": ">=1.x && <=2.x",
    "npm": "Please use yarn instead of npm"
  },
  "config": {
    "cache_outputs": ".swr",
    "test_outputs": "tests/.coverage tests/reports cypress/downloads cypress/screenshots cypress/videos",
    "port": 3000,
    "base_url": "http://localhost:3000",
    "stories_port": 6006,
    "test_group": "yarn jest --group",
    "jest_ci_flags": " --ci --reporters=default --reporters=jest-junit",
    "openssl_legacy": "cross-env NODE_OPTIONS=--openssl-legacy-provider"
  },
  "scripts": {
    "prepare": "husky install",
    "clean": "rimraf $npm_package_config_cache_outputs",
    "clean:build": "yarn clean && rimraf .next tsconfig.tsbuildinfo",
    "clean:tests": "rimraf $npm_package_config_test_outputs",
    "clean:all": "yarn clean:ci && rimraf node_modules yarn.lock",
    "prelint": "tsc --noEmit --pretty",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "sitemap": "cross-env-shell tsx scripts/sitemap.ts",
    "prebuild": "yarn clean:build",
    "build": "next build",
    "build:prod": "cross-env-shell NODE_ENV='production' NEXT_PUBLIC_CONTEXT=$CONTEXT NEXT_PUBLIC_DEPLOY_URL=$DEPLOY_URL NEXT_PUBLIC_SITE_URL=$URL yarn build",
    "build:analyzer": "cross-env-shell ANALYZE=true yarn build",
    "dev": "next dev -p $npm_package_config_port",
    "dev:proxy": "yarn concurrently \"yarn codegen\" \"yarn dev\" \"yarn proxy\"",
    "dev:debug": "cross-env-shell NODE_OPTIONS='--inspect' && yarn dev",
    "start": "next start -p $npm_package_config_port",
    "start:prod": "cross-env NODE_ENV='production' yarn start",
    "test:cypress": "cypress open",
    "test:cypress:headless": "cypress run",
    "test:e2e": "start-server-and-test dev $npm_package_config_base_url test:cypress",
    "test:e2e:headless": "start-server-and-test dev $npm_package_config_base_url test:cypress:headless",
    "test:jest": "cross-env-shell NODE_ENV='test' jest",
    "test:jest:watch": "yarn test:jest --watch",
    "test:unit:group:a11y": "$npm_package_config_test_group=unit/a11y",
    "test:unit:group:components": "$npm_package_config_test_group=unit/components",
    "test:unit:group:hooks": "$npm_package_config_test_group=unit/hooks",
    "ci:audit": "lighthouse-github-reporter",
    "ci:clean": "yarn clean:build && yarn clean:tests",
    "ci:test": "concurrently --kill-others \"yarn ci:test:unit\" \"yarn ci:test:e2e\"",
    "ci:test:e2e": "yarn test:cypress:headless",
    "ci:test:unit": "yarn test:jest $npm_package_config_jest_ci_flags",
    "proxy": "mkcert -install && mkcert localhost && npx local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "codegen": "DOTENV_CONFIG_PATH=\"./.env.development\" npx graphql-codegen --require dotenv/config --config codegen.ts",
    "create:component": "plop"
  },
  "dependencies": {
    "@axe-core/react": "^4.4.3",
    "@builder.io/partytown": "^0.6.4",
    "@dotlottie/player-component": "^1.1.0",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@netlify/plugin-nextjs": "4.41.3",
    "@next/bundle-analyzer": "^12.2.3",
    "@storyblok/react": "3.0.9",
    "@types/graceful-fs": "^4.1.5",
    "@types/html-minifier-terser": "^6.1.0",
    "@types/memory-cache": "^0.2.2",
    "@types/node": "^18.6.1",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/react-modal": "^3.13.1",
    "@types/react-paginate": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "clsx": "^1.2.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "fast-deep-equal": "^3.1.3",
    "graphql": "^16.8.1",
    "i18next": "^21.8.16",
    "keen-slider": "^6.8.5",
    "memory-cache": "^0.2.0",
    "next": "13.4.6",
    "next-i18next": "^14.0.0",
    "next-sitemap": "^4.2.3",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.0.0",
    "react-icons": "^4.4.0",
    "react-intersection-observer": "^9.7.0",
    "react-modal": "^3.15.1",
    "react-paginate": "^8.1.3",
    "react-player": "^2.10.1",
    "react-schemaorg": "^2.0.0",
    "react-tabs": "^6.0.2",
    "rimraf": "^5.0.5",
    "schema-dts": "^1.1.2",
    "sharp": "^0.30.7",
    "storyblok-rich-text-react-renderer": "^2.8.0",
    "swr": "^1.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "devDependencies": {
    "@cypress-audit/lighthouse": "^1.3.1",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/typescript-graphql-request": "^6.1.0",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@netlify/functions": "^1.6.0",
    "@svgr/webpack": "^6.3.1",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/jest-axe": "^3.5.4",
    "concurrently": "^7.3.0",
    "css-loader": "^6.7.1",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.0.0",
    "eslint": "^8.20.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-svg": "^0.0.4",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-axe": "^6.0.0",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^28.1.3",
    "jest-junit": "^14.0.0",
    "jest-preview": "^0.2.8",
    "jest-runner-groups": "^2.2.0",
    "node-fetch": "2",
    "plop": "^4.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "sass": "1.75.0",
    "sass-loader": "13.0.2",
    "sass-resources-loader": "^2.2.5",
    "start-server-and-test": "^1.14.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^28.0.7",
    "tsx": "^4.7.1"
  }
}
