{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/": ["src/*"],
      "@on/*": ["src/lib/*"],
      "@components/*": ["src/components/*"],
      "@jackets/*": ["src/jackets/*"],
      "@layouts/*": ["src/layouts/*"],
      "@pages/*": ["src/pages/*"],
      "@styles/*": ["src/styles/*"],
      "~contentful/*": ["vendors/contentful/*"],
      "~storyblok/*": ["vendors/storyblok/*"]
    },
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "incremental": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": false,
    "strictNullChecks": false,
    "target": "ESNext",
    "typeRoots": ["./node_modules/@types", "src/types"],
    "types": ["graceful-fs", "html-minifier-terser", "node", "react"]
  },
  "ts-node": {
    "compilerOptions": {
      "module": "commonjs"
    }
  },
  "include": [],
  "exclude": []
}
